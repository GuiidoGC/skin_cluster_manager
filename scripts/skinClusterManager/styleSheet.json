{
  "QDialog": {
    "border": "1px solid #5A5A5A",
    "border-radius": "3px",
    "background-color": "#2B2B2B"
  },
  "QWidget": {
    "background-color": "#2c2c2c",
    "color": "#d0d0d0",
    "font-family": "Segoe UI, Helvetica, Arial",
    "font-size": "12px"
  },
  "QPushButton": {
    "background-color": "#3c3c3c",
    "border": "none",
    "padding": "6px 12px",
    "border-radius": "0px",
    "color": "#f0f0f0"
  },
  "QPushButton:hover": {
    "background-color": "#505050",
    "border": "none",
    "border-radius": "0px"
  },
  "QPushButton:pressed": {
    "background-color": "#2a82da",
    "border": "none",
    "border-radius": "0px"
  },
  "QPushButton:disabled": {
    "background-color": "#2c2c2c",
    "color": "#777",
    "border": "none",
    "border-radius": "0px"
  },
  "QListWidget": {
    "background-color": "#262626",
    "border": "none",
    "color": "#f0f0f0",
    "padding": "4px",
    "outline": 0
  },
  "QListWidget::item": {
    "padding": "6px 8px",
    "border": "none"
  },
  "QListWidget::item:hover": {
    "background-color": "#3d3d3d"
  },
  "QListWidget::item:selected": {
    "background-color": "#2a82da",
    "color": "#ffffff"
  },
  "QComboBox": {
    "background-color": "#3C3C3C",
    "border": "1px solid #5A5A5A",
    "padding": "4px 6px",
    "border-radius": "3px",
    "color": "#FFFFFF"
  }
}
